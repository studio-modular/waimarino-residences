{
  "name": "front-end-and-cms",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation dotenv -- next dev --turbo",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation dotenv -- next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation dotenv -- payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation dotenv -- payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation dotenv -- next lint --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation dotenv -- payload",
    "payload:production": "cross-env NODE_OPTIONS=--no-deprecation dotenv -e .env.production -- payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "tsc": "cross-env NODE_OPTIONS=--no-deprecation dotenv -- tsc",
    "tsc:no-emit": "cross-env NODE_OPTIONS=--no-deprecation dotenv -- tsc --noEmit"
  },
